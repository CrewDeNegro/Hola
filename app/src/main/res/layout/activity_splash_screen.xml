<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/colorPrimary"
    android:layout_height="match_parent"
    tools:context="com.colombianapps.paulino.proyectosmartclass.SplashScreen">


    <ImageView
        android:id="@+id/fondocsbtinicial"
        android:layout_width="116dp"
        android:layout_height="105dp"
        app:srcCompat="@drawable/fondocsbtcopy"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/fondosminicial"
        android:layout_width="215dp"
        android:layout_height="36dp"
        app:srcCompat="@drawable/fondosmcopy"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/fondocsbtinicial"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="405dp"
        app:layout_constraintHorizontal_bias="0.503" />


</android.support.constraint.ConstraintLayout>
